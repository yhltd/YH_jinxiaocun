page{
  height: 100%;
  background:url('https://7968-yhltd-hsxl2-1259412419.tcb.qcloud.la/images/huaqun-bg1.jpg?sign=7c33dfb51251b632fe3fc58c227cd0fe&t=1760083177') no-repeat fixed top;
  background-size:100% 100%;
}
.iframe{
  height: 100%
}
page{
  height: 100%;
  display: flex;
  align-items: center;
}
.list{
  height: 90%;
  width: 100%;
  /* display: flex; */
  flex-direction: column;
  align-items: left;
  justify-content: space-around;  
}
.item{
  width: 73%;
  height: 7%;
  display: flex;
  align-items: center;
  background-color:rgba(213,226,220,0.7);
  border-radius: 0 10px 10px 0;
  font-size: 39rpx;
  font-weight: bold;
  color: #211f1f;
  padding-left: 30px;  
  justify-content: space-between;
  box-shadow: 10rpx 10rpx 20rpx black;
  margin-bottom: 30rpx;
}
.item_hover{
  width: 73%;
  height: 7%;
  display: flex;
  align-items: center;
  background-color:rgba(213,226,220,0.7);
  border-radius: 0 10px 10px 0;
  font-size: 39rpx;
  font-weight: bold;
  color: white;
  padding-left: 30px;  
  justify-content: space-between;
  box-shadow: 10rpx 10rpx 20rpx black;
  margin-bottom: 30rpx;
}
.img{
  width: 30px;
  height: 30px;
  margin-right: 5%;
}

.beijing_css{
  height: 100%;
}

.input_view{
  position: fixed;
  width: 90%;
  height: auto;
  top: 25%;
  background-color: white;
  border-radius: 10rpx;
  box-shadow: 10rpx 10rpx 10rpx black;
  z-index: 20;
  left:5%;
}

.mask {
  visibility: visible;
  opacity: 1;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 19;
  transition: all 0.3s ease-in-out;
}

.bt_form{
  background-color: #009688;
  width: 39%;
  margin-top: 4%;
  float: left;
  margin-left: 7%;
  color: white;
  margin-bottom: 38rpx
}