/* 标签容器样式优化 */
.van-tag{
  background: linear-gradient(135deg, #5a7592 0%, #3a4a6c 100%) !important;
  color: #ffffff !important;
  border: 1rpx solid rgba(255, 255, 255, 0.2) !important;
  margin: 2rpx 12rpx !important;
  font-size: 28rpx !important;
  font-weight: 700 !important;
}

/* 点击效果 */
.van-tag:active {
  transform: translateY(-2rpx) !important;
  box-shadow: 
    0 6rpx 16rpx rgba(42, 58, 92, 0.3),
    inset 0 1rpx 0 rgba(255, 255, 255, 0.15) !important;
  background: linear-gradient(135deg, #637e9b 0%, #42527a 100%) !important;
}

/* 关闭按钮样式 */
.van-tag .van-icon-cross {
  color: rgba(255, 255, 255, 0.8) !important;
  margin-left: 12rpx !important;
  width: 32rpx !important;
  height: 32rpx !important;
  border-radius: 50% !important;
  background: rgba(255, 255, 255, 0.1) !important;
  transition: all 0.3s ease !important;
  display: flex !important;
  align-items: center !important;
  justify-content: center !important;
}

.custom2 {
  background: linear-gradient(135deg, #5a7592 0%, #3a4a6c 100%) !important;
  color: #ffffff !important;
  border: 1rpx solid rgba(255, 255, 255, 0.2) !important;
  margin: 8rpx 12rpx !important;
  font-size: 28rpx !important;
  font-weight: 700 !important;
}

.custom2:active {
