.list{
  width: 100%;
  align-items: left;
  /* justify-content: space-around; */
  overflow: auto;
  /* position: absolute; */
  display: flex;
  flex-wrap: wrap;
  margin-top: 70rpx;  
}
.item{
  width: 300rpx;
  border-radius: 50rpx;
  height: 300rpx;
  display: flex;
  align-items: center;
  background-color: #d5e2dc;
  font-size: 48rpx;
  font-weight: bold;
  color: #ff5200;
  justify-content: center;
  box-shadow: 10rpx 10rpx 20rpx black;
  margin-bottom: 50rpx;
  background: linear-gradient(45deg, #8a8a8a 0%, #272727 100%);
  z-index: 1;
  overflow: hidden;
  position: relative;
  margin-left: 50rpx;
  margin-top: 10rpx;
}
.item_hover{
  width: 300rpx;
  border-radius: 50rpx;
  height: 300rpx;
  display: flex;
  align-items: center;
  background-color: #d5e2dc;
  font-size: 39rpx;
  font-weight: bold;
  color: #993c11;
  justify-content: center;
  box-shadow: 10rpx 10rpx 20rpx black;
  margin-bottom: 50rpx;
  background: linear-gradient(45deg, #8a8a8a 0%, #272727 100%);
  z-index: 1;
  overflow: hidden;
  position: relative;
  margin-left: 50rpx;
  transform: scale(0.9);
}
.img{
  width: 30px;
  height: 30px;
  margin-right: 5%;
}

.beijing_css{
  height: 100%;
}

.input_view{
  position: fixed;
  width: 90%;
  height: auto;
  top: 25%;
  background-color: white;
  border-radius: 10rpx;
  box-shadow: 10rpx 10rpx 10rpx black;
  z-index: 20;
  left:5%;
}

.mask {
  visibility: visible;
  opacity: 1;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 19;
  transition: all 0.3s ease-in-out;
}

.bt_form{
  background-color: #009688;
  width: 39%;
  margin-top: 4%;
  float: left;
  margin-left: 7%;
  color: white;
  margin-bottom: 38rpx
}

.rotating-bg {
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  background:conic-gradient( from 1deg,#16e498, #3df5d3, #5ee4b9, #18f7c4 );
  animation: rotate 15s linear infinite;
  z-index: -1;
}

  @keyframes rotate {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        .all{
          height: 89%;
          overflow: auto;
