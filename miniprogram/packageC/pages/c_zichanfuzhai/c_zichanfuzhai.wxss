page{
  height: 100%;
}

view {
  font-size: 30rpx;
}

button{
  font-size: 30rpx;
}
  

.table{
  display: inline-flex;
  flex-direction: column;
}

.sv_list{
  width: auto;
  white-space: nowrap;
  margin-top: 0px;
  height: 90%;
  background-color: #FCFCFC;
}

::-webkit-scrollbar-thumb {
  border-radius: 10px;
  background-color: rgba(155, 153, 153, 0.589);
}

.table_header {
  display: inline-flex;
  z-index: 1;
  height: 100rpx;
  position: sticky;
  top: 0;
}

.title{
  height: 100%;
  margin-right: 23rpx;
  display: inline-block;
  margin-left: 10px;
}

.title_text{
  display: flex;
  width: 200rpx;
  border-right: 1rpx solid #C4C5C9;
  border-bottom: 1rpx solid #C4C5C9;
  justify-content: center;
  align-items: center;
  overflow-x: auto;
  background-color: #009688;
  color: white;
  font-weight: bold;
  font-size: 35rpx;  
}

.list_row{
  display: inline-flex;
  flex-direction: row;
}

.table_main{
  display: inline-flex;
  height: 7%;
}

.list_view{
  display: flex;
  flex-direction: column;
  width: 200rpx;
  height: 70rpx;
  justify-content: center;
  align-items: center;
  background-color: white;
  color: black;
  border-right: 1rpx solid #C4C5C9;
  border-bottom: 1rpx solid #C4C5C9;
  line-height: 70rpx;  
}
.bottom_view{
  height: 10%;
}
.backSelect{
  position: fixed;
  right: 34rpx;
  bottom: 151rpx;
  height: 100rpx;
  width: 100rpx;  
}
.backSelect_image{
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  background-color: #282538;  
  box-shadow: 10rpx 10rpx 10rpx black;
}


.moreDo{
  width: 41%;
  background-color: #282B33;
  color: #E6E6E6;
  height: 75rpx;
  float: left;
  margin-top: 3%;
  margin-left: 6%;
}
.moreDo_view{
  position: fixed;
  height: 96%;
  width: 40%;
  top: 2%;
  background-color: white;
  z-index: 20;
  border-radius: 0 10rpx 10rpx 0;
  box-shadow: 10rpx 10rpx 10rpx black;  
}
.moreDo_view_bt{
  width: 90%;
  height: 100rpx;
  background-color: #009688;
  color: white;
  font-size: 37rpx;
  margin-top: 45rpx;
  line-height: 100rpx;  
}
.sw_view{
  position: fixed;
  bottom: 0;
  width: 96%;
  height: 19%;
  background-color: white;
  left: 2%;
  border-radius: 10rpx 10rpx 0 0;
  box-shadow: 0 -10rpx 10rpx black;
  z-index: 20;
}
.sw_view_btView{
  height: 38%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.sv_view_btView_bt{
  width: 18%;
  height: 60rpx;
  background-color: #009688;
  color: white;
  line-height: 60rpx;  
}
.sw_item{
  margin: auto;
  width: 96%;
  height: 115rpx;
  border: 5rpx solid #E6E6E6;
  border-radius: 10rpx;
  text-align: center;
  line-height: 115rpx;
  font-size: 37rpx;  
}

.sum_view{
  position: fixed;
  left: 5%;
  background-color: #282B33;
  z-index: 21;
  height: 28%;
  width: 78%;
  border-radius: 10rpx 10rpx 0 0;
  box-shadow: 0 -10rpx 20rpx black;
  bottom: 0;
}
.sum_view_top{
  height: 75%;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;  
}
.sum_text_view{
  margin: auto;
}
.sum_view_text{
  font-size: 35rpx;
  color: white;
}
.sum_view_text:first-child{
  font-size: 35rpx;
  color: white;
  text-shadow: 5rpx 5rpx 5rpx #E6E6E6; 
}
.sum_view_bottom{
  height: 20%;
  width: 100%;
  align-items: center;
  display: flex;  
}
.sum_bt{
  margin: auto;
  width: 33%;
  background-color: #009688;
  color: white;
  height: 60rpx;
  line-height: 60rpx;  
}


.mask{
  visibility: visible;
  opacity: 1;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  z-index: 19;
  transition: all 0.3s ease-in-out;
}