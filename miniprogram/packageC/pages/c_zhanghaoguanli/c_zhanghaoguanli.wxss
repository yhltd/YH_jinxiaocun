page{
  height: 100%;
}

view {
  font-size: 30rpx;
}

button{
  font-size: 30rpx;
}

.input_message{
  position: fixed;
  width: 80%;
  left: 10%;
  top: 4%;
  text-align: center;
  color: #787878;
  text-shadow: 5rpx 5rpx 5rpx #D6D6D6;  
}
  

.table{
  display: inline-flex;
  flex-direction: column;
}

.sv_list{
  width: auto;
  white-space: nowrap;
  margin-top: 0px;
  height: 90%;
  background-color: #FCFCFC;
}

::-webkit-scrollbar-thumb {
  border-radius: 10px;
  background-color: rgba(155, 153, 153, 0.589);
}

.table_header {
  display: inline-flex;
  z-index: 1;
  height: 100rpx;
  position: sticky;
  top: 0;
}

.title{
  height: 100%;
  margin-right: 23rpx;
  display: inline-block;
  margin-left: 10px;
}

.title_text{
  display: flex;
  width: 200rpx;
  border-right: 1rpx solid #C4C5C9;
  border-bottom: 1rpx solid #C4C5C9;
  justify-content: center;
  align-items: center;
  overflow-x: auto;
  background-color: #009688;
  color: white;
  font-weight: bold;
  font-size: 35rpx;  
}

.list_row{
