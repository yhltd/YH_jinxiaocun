 .table{
  display: inline-block; 
 }    
.th{
  display: inline-flex;
  flex-direction: row;
margin: 0;
padding: 0;
}
.tr{
   padding: 10rpx;
  display: flex;
  width: 140rpx;
  height: 55rpx;
  background-color: aliceblue;
  border-right: 1rpx solid rgb(24, 24, 26);
  border-bottom: 1rpx solid rgb(27, 27, 29);
  /* border-top:1rpx solid rgb(27, 27, 29) ; */
  justify-content: center;
  align-items: center;
  overflow-x: auto;
  font-weight: bold;
  font-size: 35rpx; 
  flex-wrap: wrap;
  
} 
