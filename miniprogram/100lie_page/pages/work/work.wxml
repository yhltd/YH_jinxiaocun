<scroll-view class="scroll" scroll-x="true"> 
<view class="section">
  <picker class="pick" mode="date" value="{{date}}" bindchange="strDateChange">
    <view class="picker">
      开始日期: {{date1}}
    </view>
  </picker>
  <picker class="pick" mode="date" value="{{date}}" bindchange="endDateChange">
    <view class="picker">
