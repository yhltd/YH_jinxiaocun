  <view class="text2">手动同步更新数据时使用此按钮</view>
  <view class="jiahao">
   <image style="width:120rpx; height:120rpx" src="../../../images/jiahao1.png"></image>
  </view>
  <button class="jia" bindtap="addhang"></button> 
</view>
</scroll-view>
